
#include<stdio.h>
int main()
{
    int a,a1,i;
    while(i<a)
    {
        scanf("%d",&a);
        //if(a1==0) break;
        a1=a*a-a+1;
        printf("%d*%d*%d=%d=%d",a,a,a,a*a*a,a1);
        for(i=1;i<a;i++)
        {
            printf("+%d",a1+i*2);
        }
    }
    printf("\n");
    return 0;
}
